<%= form_tag("/login") do %>

    <div>
        <%= label_tag :username %>
        <%= text_field_tag :username %>
    </div>
    <div>
        <%= label_tag :password %>
        <%= password_field_tag :password %>
    </div>
    <%= submit_tag "Login" %>
<% end %>

<%= link_to 'Log in with Google', '/auth/google_oauth2', method: :post %>

